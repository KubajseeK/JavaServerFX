<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="260.0" prefWidth="602.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Settings">
    <children>
        <Label layoutX="210.0" layoutY="66.0" prefHeight="45.0" prefWidth="182.0" text="Change password" textAlignment="JUSTIFY" textOverrun="CENTER_ELLIPSIS">
            <font>
                <Font size="23.0" />
            </font>
        </Label>
        <TextField fx:id="password" layoutX="227.0" layoutY="129.0" onKeyReleased="#setPass" />
        <Button fx:id="changePasswordButton" layoutX="278.0" layoutY="177.0" mnemonicParsing="false" onAction="#changePassword" text="change" />
        <Label fx:id="changeStatus" layoutX="290.0" layoutY="219.0" />
      <Label layoutX="135.0" layoutY="133.0" prefHeight="17.0" prefWidth="80.0" text="New Password:" textAlignment="CENTER" />
    </children>
</AnchorPane>
